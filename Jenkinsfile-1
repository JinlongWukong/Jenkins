pipeline {
  agent any
  stages {
    stage('Hello') {
      steps {
        sh 'echo Hello World test1'
      }
      post {
        success {
          script {
            currentBuild.result = 'SUCCESS'
          }
        }
      }
    }
  }
  post {
    always {
      echo currentBuild.result
    }
  }
}
